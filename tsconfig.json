{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"outDir": "./dist",
		"declaration": true,
		"noImplicitAny": false,
		"strictNullChecks": true,
		"strict": true
	},
	"include": [
		"src"
	]
}
